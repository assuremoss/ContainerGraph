<container>

    <image> 
        <name>$name$</name>
        <id>ea335eea17ab</id>
        <filesystem>$filesystem$</filesystem>
    </image>

    <infrastructure>
        <node>$node$</node>
        <docker_v>20.10.8</docker_v>
        <OS>$OS$</OS>
        <kernel_v>$kernel_v$</kernel_v>
        <CPUs>$CPUs$</CPUs>
        <mem>$Mem$</mem>
        <registry>$Registry$</registry>

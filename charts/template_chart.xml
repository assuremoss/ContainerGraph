<container>

    <image> 
        <id>$id$</id>
        <name>$name$</name>
        <filesystem>$filesystem$</filesystem>
    </image>

    <infrastructure>
        <hostname>$hostname$</hostname>
        <docker_v>$docker_v$</docker_v>
        <os>$os$</os>
        <kernel_v>$kernel_v$</kernel_v>
        <cpus>$cpus$</cpus>
        <mem>$mem$</mem>
        <registry>$registry$</registry>
    </infrastructure>

    <config>
        <base_image>$base_image$</base_image>
        <user>$user$</user>
        <env>$env$</env>
        <volume>$volume$</volume>
        <net_config>
            <net_adapt_type>$net_adapt_type$</net_adapt_type>
            <expose>$expose$</expose>
        </net_config>
        <entrypoint>$entrypoint$</entrypoint>
        <cmd>$cmd$</cmd>
    </config>

    <permissions>
        <files>$files$</files>
        <network>$network$</network>
        <processes>$processes$</processes>
        <adminop>$adminop$</adminop>
    </permissions>

    <timeline>
        <start_t>0</start_t>
        <stop_t>0</stop_t>
    </timeline>

</container>